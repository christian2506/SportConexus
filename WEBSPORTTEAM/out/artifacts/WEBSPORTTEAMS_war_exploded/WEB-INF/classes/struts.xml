<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="default" extends="struts-default">

        <action name="businessPartner" class="com.sportteamwebapp.actions.BusinessPartnerAction">

            <result name="error">showError.jsp</result>
        </action>


        <action name="place" class="com.sportteamwebapp.actions.PlaceAction">

            <result name="error">showError.jsp</result>
        </action>

        <action name="sport" class="com.sportteamwebapp.actions.SportAction">

            <result name="error">showError.jsp</result>
        </action>



        <action name="sportSpace" class="com.sportteamwebapp.actions.SportSpaceAction">

            <result name="error">showError.jsp</result>
        </action>


        <action name="team" class="com.sportteamwebapp.actions.TeamAction">

            <result name="error">showError.jsp</result>
        </action>


        <action name="user" class="com.sportteamwebapp.actions.UserAction">

            <result name="error">showError.jsp</result>
        </action>



        <action name="login">
            <result>validationUser.jsp</result>
        </action>
        <action name="validate" class="com.sportteamwebapp.actions.LoginAction" method="validate">
            <result name="success">indexbienvenido.jsp</result>
            <result name="error">showError.jsp</result>
        </action>




    </package>


</struts>